@model List<WebApplicationForReturnType.ViewModel.EmployeeDepartmentViewModel>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table table-condensed">

    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>City</th>
        <th>State</th>
        <th>ZipCode</th>
        <th>Department Name</th>
        <th>Delete</th>
        <th>Edit</th>
    </tr>


    @foreach (var r in Model)
    {
<tr>

    <td>@r.FirstName</td>
    <td>@r.LastName</td>
    <td>@r.City</td>
    <td>@r.State</td>
    <td>@r.ZipCode</td>
    <td>@r.DepartmentName</td>
    <td>Delete</td>
    <td>Edit</td>

</tr>

    }

</table>